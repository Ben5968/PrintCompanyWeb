{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "no-trailing-whitespace": false,
        "import-spacing": false,
        "indent": false,
        "typedef-whitespace" : false,
        "quotemark": false
    }
}
